<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="cn.mkp.dao.WebLogDao">	
 	<update id="updateNum">
 		update web_log 
 		set num = num+1		
 	</update>
 	<select id="getWebLog" resultType="cn.mkp.entity.WebLog">
 		select * from web_log
 	</select>
 </mapper>